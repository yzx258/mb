<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rbgt.mb.mapper.UserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.rbgt.mb.domain.User">
        <id column="id" property="id" />
        <result column="create_time" property="createTime" />
        <result column="created_by" property="createdBy" />
        <result column="last_modified_by" property="lastModifiedBy" />
        <result column="update_time" property="updateTime" />
        <result column="name" property="name" />
        <result column="sex" property="sex" />
        <result column="status" property="status" />
        <result column="chat_flag" property="chatFlag" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, create_time AS createTime, created_by AS createdBy, last_modified_by AS lastModifiedBy, update_time AS updateTime, name, sex, status, chat_flag AS chatFlag
    </sql>

</mapper>
